					<div class="col-sm-2">
						{{ fields.field_radio('soconfig_advanced_store','scsscompile', {1:'ON',0 : 'OFF'},0,'parent scsscompile' )}} 
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label" >CSS Format</label>
