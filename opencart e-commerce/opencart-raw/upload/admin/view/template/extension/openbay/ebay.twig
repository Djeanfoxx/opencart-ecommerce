{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <h1>{{ text_dashboard }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    {% if success %}
    <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}</div>
    {% endif %}
    <div class="row">
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_settings }}" id="settings-link"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-wrench fa-stack-1x"></i></span>
          <h4>{{ text_heading_settings }}</h4>
          </a> </div>
      </div>
      {% if validation == true %}
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_sync }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-cog fa-stack-1x"></i></span>
          <h4>{{ text_heading_sync }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_subscribe }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-user fa-stack-1x"></i></span>
          <h4>{{ text_heading_subscription }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_usage }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-bar-chart-o fa-stack-1x"></i></span>
          <h4>{{ text_heading_usage }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_itemlink }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-link fa-stack-1x"></i></span>
          <h4>{{ text_heading_links }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_itemimport }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-cloud-download fa-stack-1x"></i></span>
          <h4>{{ text_heading_item_import }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_orderimport }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-download fa-stack-1x"></i></span>
          <h4>{{ text_heading_order_import }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_summary }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-bar-chart-o fa-stack-1x"></i></span>
          <h4>{{ text_heading_summary }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_profile }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-file-text fa-stack-1x"></i></span>
          <h4>{{ text_heading_profile }}</h4>
          </a> </div>
      </div>
      <div class="col-md-3 text-center">
        <div class="well"> <a href="{{ links_template }}"> <span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-code fa-stack-1x"></i></span>
          <h4>{{ text_heading_template }}</h4>
          </a> </div>
      </div>
      {% else %}
      <div class="col-md-3 text-center">
        <div class="well"><a href="{{ link_signup }}" target="_BLANK"><span class="fa-stack fa-3x"> <i class="fa fa-square-o fa-stack-2x"></i> <i class="fa fa-star fa-stack-1x"></i></span>
          <h4>{{ text_heading_register }}</h4>
          </a> </div>
      </div>
      {% endif %}
    </div>
  </div>
</div>
{{ footer }}
